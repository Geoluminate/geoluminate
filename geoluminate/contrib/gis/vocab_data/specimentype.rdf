<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/corePiece">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core piece</skos:prefLabel>
    <skos:definition>Material occurring between unambiguous [as curated] breaks in recovery.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/individualSample">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Individual sample</skos:prefLabel>
    <skos:definition>A sample that is an individual unit, including rock hand samples, a biological specimen, or a bottle of fluid.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/standardReferenceSpecimen">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Standard reference specimen</skos:prefLabel>
    <skos:definition>Standard reference specimen</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/grab">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Grab</skos:prefLabel>
    <skos:definition>A sample (sometimes mechanically collected) from a deposit or area, not intended to be representative of the deposit or area.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/rockPowder">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Rock powder</skos:prefLabel>
    <skos:definition>A sample created from pulverizing a rock to powder.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/cuttings">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Cuttings</skos:prefLabel>
    <skos:definition>Loose, coarse, unconsolidated material suspended in drilling fluid.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/dredge">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Dredge</skos:prefLabel>
    <skos:definition>A group of rocks collected by dragging a dredge along the seafloor.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreSection">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core section</skos:prefLabel>
    <skos:definition>Arbitrarily cut segments of a "core"</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreSectionHalf">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core section half</skos:prefLabel>
    <skos:definition>Half-cylindrical products of along-axis split of a section or its component fragments through a selected diameter.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreQuarterRound">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core quarter round</skos:prefLabel>
    <skos:definition>Quarter-cylindrical products of along-axis split of a half round.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/forestFloorDigestion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Forest floor digestion</skos:prefLabel>
    <skos:definition>Sample that consists of a digestion of forest floor material</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/theSpecimenTypeIsUnknown">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>The specimen type is unknown</skos:prefLabel>
    <skos:definition>The specimen type is unknown</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/orientedCore">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Oriented core</skos:prefLabel>
    <skos:definition>Core that can be positioned on the surface in the same way that it was arranged in the borehole before extraction.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/other">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Other</skos:prefLabel>
    <skos:definition>Sample does not fit any of the existing type designations. It is expected that further detailed description of the particular sample be provided.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/litterFallDigestion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Litter fall digestion</skos:prefLabel>
    <skos:definition>Sample that consists of a digestion of litter fall</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreSub-Piece">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core sub-piece</skos:prefLabel>
    <skos:definition>Unambiguously mated portion of a larger piece noted for curatorial management of the material.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/precipitationBulk">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Precipitation bulk</skos:prefLabel>
    <skos:definition>Sample from bulk precipitation</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/automated">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Automated</skos:prefLabel>
    <skos:definition>Sample collected using an automated sampler.</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/foliageDigestion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Foliage digestion</skos:prefLabel>
    <skos:definition>Sample that consists of a digestion of plant foliage</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/petriDishDryDeposition">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Petri dish (dry deposition)</skos:prefLabel>
    <skos:definition>Sample from dry deposition in a petri dish</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreWholeRound">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core whole round</skos:prefLabel>
    <skos:definition>Cylindrical segments of core or core section material.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/coreHalfRound">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core half round</skos:prefLabel>
    <skos:definition>Half-cylindrical products of along-axis split of a whole round</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <dc:title>ODM2 Specimen Type Controlled Vocabulary</dc:title>
    <dc:creator>ODM2 Working Group</dc:creator>
    <dc:description>A vocabulary for describing the type of a physical Specimen.</dc:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/biota">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Biota</skos:prefLabel>
    <skos:definition>A specimen consisting of all or part of a biological organism, including flora or fauna.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/core">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Core</skos:prefLabel>
    <skos:definition>Long cylindrical cores</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/terrestrialSection">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Terrestrial section</skos:prefLabel>
    <skos:definition>A sample of a section of the near-surface Earth, generally in the critical zone.</skos:definition>
    <skos:historyNote>SESAR Sample type CV. See http://www.geosamples.org/help/vocabularies#object.</skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/foliageLeaching">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Foliage leaching</skos:prefLabel>
    <skos:definition>Sample that consists of leachate from foliage</skos:definition>
    <skos:historyNote>Originally from CUAHSI SampleTypeCV. See: http://his.cuahsi.org/mastercvreg/edit_cv11.aspx?tbl=SampleTypeCV. </skos:historyNote>
  </rdf:Description>
  <rdf:Description rdf:about="http://vocabulary.odm2.org/specimentype/thinSection">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://vocabulary.odm2.org/specimentype"/>
    <skos:prefLabel>Thin section</skos:prefLabel>
    <skos:definition>Thin section</skos:definition>
  </rdf:Description>
</rdf:RDF>
